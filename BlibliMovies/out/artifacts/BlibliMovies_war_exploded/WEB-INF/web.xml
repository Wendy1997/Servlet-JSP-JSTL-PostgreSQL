<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>Controller.StoreController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index.jsp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>store_login</servlet-name>
        <servlet-class>Controller.StoreController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>store_login</servlet-name>
        <url-pattern>/store_login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>account_login</servlet-name>
        <servlet-class>Controller.AccountController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>account_login</servlet-name>
        <url-pattern>/account_login</url-pattern>
    </servlet-mapping>
</web-app>



